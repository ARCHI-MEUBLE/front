{
  "version": 2,
  "name": "archimeuble-frontend",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "env": {
    "NEXT_PUBLIC_API_URL": "https://archimeuble-backend-staging.railway.app"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_API_URL": "https://archimeuble-backend-staging.railway.app"
    }
  },
  "regions": ["cdg1"],
  "functions": {
    "api/**/*.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
